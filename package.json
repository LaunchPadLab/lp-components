{
  "name": "@launchpadlab/lp-components",
  "version": "1.26.3",
  "description": "Our Components",
  "main": "lib/index.js",
  "repository": "launchpadlab/lp-components",
  "homepage": "https://github.com/launchpadlab/lp-components",
  "author": {
    "name": "Dave Corwin",
    "email": "dave@launchpadlab.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "yarn run build:development",
    "build": "babel src --out-dir lib",
    "build:development": "babel src --watch --out-dir lib",
    "clean": "rimraf lib",
    "docs": "documentation build src/index.js -f md -o docs.md",
    "lint": "eslint src",
    "precommit": "yarn run docs && git add docs.md",
    "prepublish": "yarn run clean && yarn run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o .storybook/static/storybook",
    "heroku-prebuild": "yarn remove node-sass",
    "heroku-postbuild": "yarn run storybook:build"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "lib/"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "bourbon": "^4.3.3",
    "bourbon-neat": "^1.8.0",
    "css-loader": "^0.27.3",
    "documentation": "^4.0.0-beta.18",
    "enzyme": "^2.8.2",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.1",
    "react": "^15.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0"
  },
  "peerDependencies": {
    "react": "^15.5.0"
  },
  "dependencies": {
    "@launchpadlab/lp-utils": "^2.12.1",
    "classnames": "^2.2.5",
    "codeclimate-test-reporter": "^0.4.1",
    "husky": "^0.13.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react-datepicker": "^0.46.0"
  }
}
